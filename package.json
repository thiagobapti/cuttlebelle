			"url": "https://dominik-wilkowski.com/"
		}
	],
	"repository": {
		"type": "git",
		"url": "git://github.com/cuttlebelle/cuttlebelle.git"
	},
	"bugs": {
		"url": "https://github.com/cuttlebelle/cuttlebelle/issues"
	},
	"licenses": [
		{
			"type": "GPL-3.0",
			"url": "https://github.com/cuttlebelle/cuttlebelle/blob/master/LICENSE"
		}
	],
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"build": "yarn build:js",
		"build:js": "babel src --out-dir dist",
		"build:watch": "yarn build:js",
		"watch": "yarn build && onchange 'src/*.js' -- yarn build:watch",
		"nuke": "rm -rf dist && rm -rf node_modules && rm yarn.lock"
	},
	"dependencies": {
		"@babel/core": "^7.7.7",
		"@babel/plugin-proposal-object-rest-spread": "^7.7.7",
		"@babel/plugin-syntax-dynamic-import": "^7.7.4",
		"@babel/plugin-transform-runtime": "^7.7.6",
		"@babel/preset-env": "^7.7.7",
		"@babel/preset-react": "^7.7.4",
		"@babel/register": "^7.7.7",
		"@babel/runtime": "^7.7.7",
		"autoprefixer": "^9.7.5",
		"babel-plugin-import-redirect": "^1.1.1",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
		"babelify": "^10.0.0",
		"browser-sync": "^2.26.7",
		"browserify": "^16.5.1",
		"del": "^5.1.0",
		"fs-extra": "^8.1.0",
		"js-yaml": "^3.13.1",
		"marked": "^0.8.0",
		"node-notifier": "^6.0.0",
		"node-sass": "^4.13.1",
		"postcss": "^7.0.27",
		"prettify-html": "^0.0.2",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-docgen": "^5.0.0",
		"react-dom": "^16.12.0",
		"require-from-string": "^2.0.2",
		"shortid": "^2.2.15",
		"sitemap": "^6.1.4",
		"slugify": "^1.3.6",
		"traverse": "^0.6.6",
		"uglifyify": "^5.0.2",
		"window-size": "^1.1.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.7.7",
		"chalk": "^3.0.0",
		"copy-dir": "^1.2.0",
		"diff": "^4.0.1",
		"dirsum": "^0.1.1",
		"flow-bin": "^0.114.0",
		"onchange": "^6.1.0",
		"replace-in-file": "^5.0.2"
	},
	"keywords": [
		"static site generator",
		"react.js",
		"editing",
		"CLI",
		"rapid prototying",
		"pretty"
	],
	"files": [
		"dist/*",
		"assets/*",
		".babelrc"
	],
	"main": "dist/index.js",
	"bin": {
		"cuttlebelle": "./dist/index.js"
	},
	"license": "GPL-3.0"
}
